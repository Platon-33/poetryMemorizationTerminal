    case 13://The Quality of Mercy
        string mercyOne = "The quality of mercy is not strained;" +
            "\r\nIt droppeth as the gentle rain from heaven" +
            "\r\nUpon the place beneath. It is twice blest;" +
            "\r\nIt blesseth him that gives and him that takes:" +
            "\r\n'T is mightiest in the mightiest; it becomes" +
            "\r\nThe throned monarch better than his crown:";

        string mercyTwo = "\r\nHis sceptre shows the force of temporal power," +
            "\r\nThe attribute to awe and majesty," +
            "\r\nWherein doth sit the dread and fear of kings;" +
            "\r\nBut mercy is above this sceptred sway;" +
            "\r\nIt is enthronèd in the hearts of kings," +
            "\r\nIt is an attribute to God himself;";

        string mercyThree = "\r\nAnd earthly power doth then show likest God's" +
            "\r\nWhen mercy seasons justice. Therefore, Jew," +
            "\r\nThough justice be thy plea, consider this," +
            "\r\nThat, in the course of justice, none of us" +
            "\r\nShould see salvation: we do pray for mercy;" +
            "\r\nAnd that same prayer doth teach us all to render" +
            "\r\nThe deeds of mercy.";

        Console.WriteLine("The Quality of Mercy Is Not Strained by William Shakespeare");
        Console.ReadKey();
        Console.WriteLine(mercyOne);
        Console.ReadKey();
