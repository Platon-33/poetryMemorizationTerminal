    case 16://For Once Then, Something
        string somethingOne = "Others taunt me with having knelt at well-curbs" +
        "\r\nAlways wrong to the light, so never seeing" +
        "\r\nDeeper down in the well than where the water" +
        "\r\nGives me back in a shining surface picture" +
        "\r\nMe myself in the summer heaven godlike" +
        "\r\nLooking out of a wreath of fern and cloud puffs.";
       
        string somethingTwo = "\r\nOnce, when trying with chin against a well - curb,"+
        "\r\nI discerned, as I thought, beyond the picture,"+
        "\r\nThrough the picture, a something white, uncertain,"+
        "\r\nSomething more of the depths—and then I lost it."+
        "\r\nWater came to rebuke the too clear water.";
       
        string somethingThree = "\r\nOne drop fell from a fern, and lo, a ripple"+
        "\r\nShook whatever it was lay there at bottom,"+
        "\r\nBlurred it, blotted it out. What was that whiteness?"+
        "\r\nTruth? A pebble of quartz? For once, then, something.";

        Console.WriteLine("For Once Then, Something by Robert Frost");
        Console.ReadKey();
        Console.WriteLine(somethingOne);
        Console.ReadKey();
        Console.WriteLine(somethingTwo);
        Console.ReadKey();
        Console.WriteLine(somethingThree);
